{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Enable google view image button",
  "description": "Append [View Image] to Google search page. coded with love in http://webafrooz.com",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*"
      ],
      "js": [
        "bundle.js",
        "append_view.js"
      ]
    }
  ]
}